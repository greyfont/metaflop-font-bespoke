% box dimension definition %
beginadjichar("l",0.2,asc#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.4u#,space+0.4u#);

% construction %
x1=x2=leftstemloc;
x1l=x2l=x1-(cont*px/2);
y1=0;
y2=h;

% penpositions %
penpos1(whatever,0);
penpos2(whatever,0);

% draw %
penstroke z1e---z2e;

penlabels(range 1 thru 150);

endchar;