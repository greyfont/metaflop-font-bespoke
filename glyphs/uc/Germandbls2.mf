u:=ypart((z99--z98) intersectiontimes (super_ql(15r,14r)--z13r));
b:=subpath (infinity,u) of (super_ql(15r,14r)--z13r);
fill a--z13l--b--cycle;

penstroke z23e--super_qr(24e,25e)--super_ql(26e,27e)--z28e;
