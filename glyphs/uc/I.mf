% box dimension definition %
beginadjichar("I",0.2,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.6u#,space+0.6u#);

% construction %
x1=x2=leftstemloc;
x1l=x2l=x1-(cont*px/2);
y1=0;
y2=h;

% penpositions %
pos1(whatever,up,0);
pos2(whatever,up,0);

% draw %
penstroke z1e--z2e;

% composite glyphs %
charanchortops_[charcode]=(.5w,1.25h+.5px);
charanchorbots_[charcode]=(.5w,0);

penlabels(range 1 thru 150);

endchar;