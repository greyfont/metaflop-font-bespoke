% box dimension definition %
beginadjchar(47,3.75,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.0u#,space+0.0u#);

% construction %
x1l=leftstemloc-(cont*px/2);
x2r=w-x1l;
y2=1.05h;
y1=-.05h;

% penpositions %
pos1(cont*px,(x2r-x1l-px,h),0);
pos2(cont*px,(x2r-x1l-px,h),0);

% draw %
penstroke z1e--z2e;

penlabels(range 1 thru 150);

endchar;