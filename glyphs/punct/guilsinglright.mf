% box dimension definition %
beginadjchar(15,2.25,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.25u#,space+0.25u#);

% construction %
x2l=x4l=leftstemloc-cont*px/2;
x1r=w-x2l;
y1=.5h;
y4=h;
y2=0;

% penpositions %
pos1(cont*px,(x1r-x2l,h),0);
pos2(cont*px,(x1r-x2l,h),0);
pos4(cont*px,(x1r-x2l,h),0);

% draw %
penstroke z1e---z2e;
penstroke z1e---z4e;

penlabels(range 1 thru 150);

endchar;