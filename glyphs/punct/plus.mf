% box dimension definition %
beginadjchar(43,4.5,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.3u#,space+0.3u#);

% construction %
x5=leftstemloc-(cont*px/2);
x6=w-x5;
x1=x2=0.5*w;
x1l=x2l=x1-(cont*px/2);
y1=mean;
y2=0;
y5=y6=0.5*mean;
y5l=y6l=y5-px/2;

% penpositions %
pos1(whatever,up,0);
pos2(whatever,up,0);
pos5(whatever,right,90);
pos6(whatever,right,90);

% draw %
penstroke z1e---z2e;
penstroke z5e---z6e;

penlabels(range 1 thru 150);

endchar;