% box dimension definition %
beginadjchar(14,2.25,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.25u#,space+0.25u#);

% construction %
x1l=leftstemloc-cont*px/2;
x2r=x4r=w-x1l;
y1=.5h;
y4=h;
y2=0;

% penpositions %
pos1(cont*px,(x2r-x1l,h),0);
pos2(cont*px,(x2r-x1l,h),0);
pos4(cont*px,(x2r-x1l,h),0);

% draw %
penstroke z1e---z2e;
penstroke z1e---z4e;

penlabels(range 1 thru 150);

endchar;