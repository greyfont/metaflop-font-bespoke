% box dimension definition %
beginadjchar(61,4.5,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.2u#,space+0.2u#);

% construction %
x1=x5=leftstemloc-(cont*px/2);
x2=x6=w-x5;
y1=y2=0.7*mean;
y5=y6=0.3*mean;
y1l=y2l=y1-px/2;
y5l=y6l=y5-px/2;

% penpositions %
pos1(whatever,right,90);
pos2(whatever,right,90);
pos5(whatever,right,90);
pos6(whatever,right,90);

% draw %
penstroke z1e---z2e;
penstroke z5e---z6e;

penlabels(range 1 thru 150);

endchar;