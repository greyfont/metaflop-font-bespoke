% box dimension definition %
beginadjchar(45,3.25,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.5u#,space+0.5u#);

% construction %
x5=leftstemloc;
x6=w-x5;
y5=y6=0.8*mean;
y5l=y6l=y5-px/2;

% penpositions %
penpos5(whatever,90);
penpos6(whatever,90);

% draw %
penstroke z5e---z6e;

penlabels(range 1 thru 150);

endchar;