% box dimension definition %
beginadjichar(124,0.2,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.6u#,space+0.6u#);

% construction %
x1=x2=leftstemloc;
x1l=x2l=x1-(cont*px/2);
y2=1.05h;
y1=-.05h;

% penpositions %
pos1(whatever,up,0);
pos2(whatever,up,0);

% draw %
penstroke z1e--z2e;

% composite anchor %
charanchorbots_[charcode]=(.5w,0);

penlabels(range 1 thru 150);

endchar;