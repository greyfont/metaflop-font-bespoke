% box dimension definition %
beginadjchar(127,2.75,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.2u#,space+0.2u#);

% construction %
x5=leftstemloc-(cont*px/2);
x6=w-x5;
y5=y6=0.5*mean;
y5l=y6l=y5-px/2;

% penpositions %
pos5(whatever,right,90);
pos6(whatever,right,90);

% draw %
penstroke z5e---z6e;

penlabels(range 1 thru 150);

endchar;