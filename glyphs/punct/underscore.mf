% box dimension definition %
beginadjchar(95,4.5,mean#,des#);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.1u#,space+0.1u#);

% construction %
x5=leftstemloc-(cont*px/2);
x6=w-x5;
y5=y6=y5r-px/2;
y5r=y6r=-0.25d;

% penpositions %
pos5(whatever,right,90);
pos6(whatever,right,90);

% draw %
penstroke z5e--z6e;

penlabels(range 1 thru 150);

endchar;