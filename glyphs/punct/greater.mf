% box dimension definition %
beginadjchar(62,4.5,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.2u#,space+0.2u#);

% construction %
x2=x6=leftstemloc-(cont*px/2);
x1=w-x2;
y1=0.5*mean;
y2=mean-px/2;
y6=0+px/2;

% penpositions %
pos1(cont*px,(x1-x2,.4h),90);
pos2(cont*px,(x1-x2,.4h),90);
pos6(cont*px,(x1-x2,.4h),90);

% draw %
penstroke z1e---z2e;
penstroke z1e---z6e;

penlabels(range 1 thru 150);

endchar;