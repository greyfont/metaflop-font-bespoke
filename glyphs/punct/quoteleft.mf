% box dimension definition %
beginadjchar(96,1.75,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.4u#,space+0.4u#);

% construction %
x4=leftstemloc;
x3=w-leftstemloc;
y3=h;
y4=.7h;

% penpositions %
pos3(cont*px,(x3-x4,.45h),0);
pos4(cont*px,(x3-x4,.45h),0);

% draw %
penstroke z3e--z4e;

penlabels(range 1 thru 150);

endchar;