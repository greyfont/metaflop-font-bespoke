% box dimension definition %
beginadjichar(25,0.2,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.45u#,space+0.45u#);

% construction %
x1=x2=leftstemloc;
x1l=x2l=x1-(cont*px/2);
y1=0;
y2=mean;

% penpositions %
penpos1(whatever,0);
penpos2(whatever,0);

% draw %
penstroke z1e..z2e;

% composite glyphs %
charanchortops_[charcode]=(.5w,1.4h);

penlabels(range 1 thru 150);

endchar;