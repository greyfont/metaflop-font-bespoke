% box dimension definition %
beginadjchar(25,0.5,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.45u#,space+0.45u#);

% construction %
x1=x2=.5w;
x1l=x2l=x1-(cont*px/2);
y1=0;
y2=mean;

% penpositions %
pos1(whatever,up,0);
pos2(whatever,up,0);

% draw %
penstroke z1e..z2e;

% composite glyphs %
charanchortops_[charcode]=(.5w,1.525h);

penlabels(range 1 thru 150);

endchar;