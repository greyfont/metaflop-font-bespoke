% box dimension definition %
beginadjchar(16,3,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.8u#,space+0.8u#);

% construction %
x1=.075w;
x2=.35w;
x4=.65w;
x3=.925w;
y2=y3=h;
y1=y4=.675h;

% penpositions %
pos1(cont*px,(x1-x2,.45h),0);
pos2(cont*px/1.35,(x1-x2,.45h),0);
pos3(cont*px/1.35,(x3-x4,.45h),0);
pos4(cont*px,(x3-x4,.45h),0);

% draw %
penstroke z1e--z2e;
penstroke z3e--z4e;

penlabels(range 1 thru 150);

endchar;