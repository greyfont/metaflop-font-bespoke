% box dimension definition %
beginadjichar(44,0.2,mean#,des#);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.5u#,space+0.5u#);

% construction %
x1=x2=leftstemloc;
x1l=x2l=x1-(cont*px/2);
y1=.1*mean+px;
y2=-.3*des;

% penpositions %
penpos1(whatever,0);
penpos2(whatever,0);

% draw %
penstroke z1e---z2e;

penlabels(range 1 thru 150);

endchar;