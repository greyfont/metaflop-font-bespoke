% box dimension definition %
beginadjchar(21,4.5,mean#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.2u#,space+0.2u#);

% construction %
x5=leftstemloc-(cont*px/2);
x6=w-x5;
y5=y6=0.5*mean;
y5l=y6l=y5-px/2;

% penpositions %
penpos5(whatever,90);
penpos6(whatever,90);

% draw %
penstroke z5e---z6e;

penlabels(range 1 thru 150);

endchar;