% box dimension definition %
beginadjchar(13,1.75,cap#,des#);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.4u#,space+0.4u#);

% construction %
x4=.25w;
x3=.725w;
y3=0.125h;
y4=-0.2h;

% penpositions %
pos3(cont*px,(x3-x4,.45h),0);
pos4(cont*px/1.35,(x3-x4,.45h),0);

% draw %
penstroke z3e--z4e;

penlabels(range 1 thru 150);

endchar;