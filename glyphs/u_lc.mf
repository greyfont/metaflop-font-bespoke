%%% box dimension definition %%%
beginadjchar("u",3.75,mean#,0);

%%% sidebearings %%%
numeric l,r;
normal_adjust_fit(0.6u#,0.7u#);

%%% overshoot %%%
y8r=y7r=px -o; % overshoot down
bot y8=0 -o; % overshoot down
bot y7=0 -o; % overshoot down

%%% construction %%%
x8r=if (incx>.5): .5w else: x1r+incx*(x6r-x1r) fi;
x7r=if (incx>.5): .5w else: x6r-incx*(x6r-x1r) fi;
y1r=px+incy*((h-px)-px);
if (y1r>3/8h): y1r:=3/8h; fi
y2=y3=h;
x2=x1=x2l+.5(cont*px);
x3=w-x1;
x3=x4=x3l-.5(cont*px);
x3l=x4l;
y4=0;
x1=leftstemloc;
x6=w-x1;
y6r=y1r;
x1r=x1+(cont*px/2);
x6r=x6-(cont*px/2);
ang:=angle(z6r-z7r);

%%% penpositions %%%
penpos1(whatever,if (superness=.5): -(ang-90)/2 else: 0 fi);
penpos2(whatever,0);
penpos3(whatever,180);
penpos4(whatever,180);
penpos6(whatever,if (superness=.5): (ang-90)/2 else: 180 fi);
penpos7(whatever,if (superness=.5): -(180-ang)/2 else: 90 fi);
penpos8(whatever,if (superness=.5): (180-ang)/2 else: 90 fi);

%%% draw %%%
penstroke super_ql(6e,7e)---super_qr(8e,1e)---z2e;
penstroke z3e---z4e;
if (cont>1.1): 
	x6l:=x6r+px; fi

% penlabels(1,2,3,4,5,6,7,8);

endchar;