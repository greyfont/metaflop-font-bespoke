%%% box dimension definition %%%
beginadjchar("x",3.9,mean#,0);

%%% sidebearings %%%
numeric l,r;
normal_adjust_fit(-0.1u#,-0.1u#);

%%% construction %%%
x1l=leftstemloc-cont*px/2;
x2r=w-x1l;
x3l=x1l;
x4r=x2+.5(x2r-x2);
y1=y4=h;
y2=y3=0;
alphaxx1=diag_ratio(1,cont*px,h,x2r-x1l);
alphaxx2=diag_ratio(1,px,h,x2r-x1l);

%%% penpositions %%%
penpos1(alphaxx1*cont*px,0);
penpos2(alphaxx1*cont*px,0);
penpos3(alphaxx2*px,0);
penpos4(alphaxx2*px,0);

%%% draw %%%
penstroke z1e..z2e;
penstroke z3e..z4e;

% penlabels(range 1 thru 30);

endchar;