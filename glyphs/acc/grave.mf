% box dimension definition %
beginadjchar(0,3,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space+0.1u#,space-1.0u#);

% construction %
x3=leftstemloc;
x4=0.5w;
y4=0.8h;
y3=h;

% penpositions %
pos3(cont*px,(x3-x4,.2h),0);
pos4(cont*px/1.35,(x3-x4,.2h),0);

% draw %
penstroke z3e--z4e;

% composite anchor %
charanchortops_[charcode]=(.5w,h);

penlabels(range 1 thru 150);

endchar;