% box dimension definition %
beginadjchar(9,3,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space-0.1u#,space-0.1u#);

% construction %
x1=leftstemloc;
x2=w-leftstemloc;
y1=y2=h-px;
y1l=y2l=y1-px/2;

% penpositions %
pos1(whatever,right,90);
pos2(whatever,right,90);

% draw %
penstroke z1e--z2e;

% composite anchor %
charanchortops_[charcode]=(.5w,h);

penlabels(range 1 thru 150);

endchar;