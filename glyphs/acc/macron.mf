% box dimension definition %
beginadjchar(9,3,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space-0.45u#,space-0.45u#);

% construction %
x1=leftstemloc/2;
x2=w-x1;
y1=y2=0.875h;
y1l=y2l=y1-px/2.25;

% penpositions %
pos1(whatever,right,90);
pos2(whatever,right,90);

% draw %
penstroke z1e--z2e;

% composite anchor %
charanchortops_[charcode]=(.5w,h);

penlabels(range 1 thru 150);

endchar;