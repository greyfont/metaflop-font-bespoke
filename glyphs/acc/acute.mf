% box dimension definition %
beginadjchar(1,2.25,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*-0.1u#,space*-0.1u#);

% construction %
x4=leftstemloc+px/2;
x3=w-leftstemloc;
y4=0.85h;
y3=h;

% penpositions %
pos3(cont*px,(x4-x3,.2h),0);
pos4(cont*px,(x4-x3,.2h),0);

% draw %
penstroke z3e--z4e;

% composite anchor %
charanchortops_[charcode]=(.5w,h);

penlabels(range 1 thru 150);

endchar;