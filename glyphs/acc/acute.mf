% box dimension definition %
beginadjchar(1,3,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space-0.85u#,space-0.15u#);

% construction %
x4=0.45w;
x3=0.775w;
y4=0.8h;
y3=h;

% penpositions %
pos3(cont*px,(x4-x3,.2h),0);
pos4(cont*px/1.35,(x4-x3,.2h),0);

% draw %
penstroke z3e--z4e;

% composite anchor %
charanchortops_[charcode]=(.5w,h);

penlabels(range 1 thru 150);

endchar;