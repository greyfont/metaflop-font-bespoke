% MetaFlop
% Standard 1.3

beginadjichar(",",0.2, mean#, 0);

%%% sidebearings %%%
numeric l,r;
normal_adjust_fit(0.5u#,0.5u#);


x4=x3=leftstemloc;
y4-y3=1.2*cont*px;
y3=0;
y5=y6=.5(y4+y3);
x5-x6=1.2*cont*px;
.5[x5,x6]=leftstemloc;

x7=leftstemloc; y7=y6;


.5[x6,x7]=x8=x9;
y8=-y4;
y9=y8-epsilon;
fill z7---super_ql(5,9)---z8---super_qr(8,7)---cycle;
fill super_qr(4,5)---super_ql(5,3)---super_qr(3,6)---super_ql(6,4)---cycle;

penlabels(range 1 thru 30);

endchar;
